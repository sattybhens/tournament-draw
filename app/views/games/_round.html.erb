<div class="span-3<%= " last" if round == max_rounds %>">
	<br>Round <%= round %></br>
	<%- (1..size_of_round).each do |num|%>
	<%- decorate = cycle('odd','even') %>
	<span class="round-<%= round %> <%= decorate %>-above"></span>
	<p class="<%= decorate %>"><%= "Player #{num}" %></p>
	<span class="round-<%= round %> <%= decorate %>-below"></span>
	<%- end %>
</div>